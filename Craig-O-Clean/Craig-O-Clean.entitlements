<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Automation permission for browser tab management -->
	<key>com.apple.security.automation.apple-events</key>
	<true/>
	<!-- Temporary exception for AppleScript execution -->
	<key>com.apple.security.temporary-exception.apple-events</key>
	<array>
		<string>com.apple.Safari</string>
		<string>com.google.Chrome</string>
		<string>com.microsoft.edgemac</string>
		<string>com.brave.Browser</string>
		<string>com.apple.systemevents</string>
	</array>
	<!-- App Sandbox disabled for system monitoring -->
	<key>com.apple.security.app-sandbox</key>
	<false/>
	<!-- Hardened runtime with exceptions -->
	<key>com.apple.security.cs.allow-unsigned-executable-memory</key>
	<false/>
	<key>com.apple.security.cs.disable-library-validation</key>
	<false/>
</dict>
</plist>
